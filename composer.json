{
    "name": "leonardo-max/livewire-kanban-board",
    "description": "Livewire component to show models/data according to its current status on a Kanban board",
    "keywords": [
        "leonardo-max",
        "livewire-kanban-board"
    ],
    "homepage": "https://github.com/leonardo-max/livewire-kanban-board",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Pieter Naber",
            "email": "Pieter@Mantix.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^11.0",
        "laravel/framework": "^11.0",
        "livewire/livewire": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "LivewireKanbanBoard\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LivewireKanbanBoard\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LivewireKanbanBoard\\LivewireKanbanBoardServiceProvider"
            ],
            "aliases": {
                "LivewireKanbanBoard": "LivewireKanbanBoard\\LivewireKanbanBoardFacade"
            }
        }
    }
}
